import java.util.*;
class HelloWorld {
    public static void occur(String xiom)
    {
        Set<Character> ch = new HashSet<>();
        for(int i =0;i<xiom.length();i++)
        {
            if(!ch.contains(xiom.charAt(i)))
            {
                ch.add(xiom.charAt(i));
                
            }
            else
            {
                System.out.print(xiom.charAt(i));
            }
        }
    }
    public static void main(String[] args) {
        String s = "Satya Swarup";
        occur(s);
        
    }
}
