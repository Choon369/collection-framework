import java.util.*;
class HelloWorld {
    public static String occur(String xiom)
    {
        String s ="";
        Set<Character> ch = new HashSet<>();
        for(int i =0;i<xiom.length();i++)
        {
            if(!ch.contains(xiom.charAt(i)))
            {
                ch.add(xiom.charAt(i));
                 s =  s + xiom.charAt(i);
            }
            
        }
        return s;
    }
    public static void main(String[] args) {
        String s = "Satya Swarup";
        System.out.println(occur(s));
        
    }
}
